                        <script>

                            var DeleteComment = function () {
                                $("#loaderCommentDiv").show();
                                var commentId = $("#hiddenCommentId").val();
                                $.ajax({
                                    type: "POST",
                                    url: "/User/DeleteComment",
                                    data: { Id: commentId },
                                    success: function (result) {
                                        $("#loaderCommentDiv").hide();
                                        debugger
                                        $("#CommentModel").modal('hide');
                                        $("#rowComment_" + commentId).remove();
                                    }
                                })
                            }
                            var DeleteQuestion = function () {
                                $("#loaderQuestionDiv").show();
                                var queId = $("#hiddenQuestionId").val();
                                $.ajax({
                                    type: "POST",
                                    url: "/User/DeleteQuestion",
                                    data: { Id: queId },
