                                        $("#loaderQuestionDiv").hide();
                                        debugger
                                        $("#QuestionModal").modal('hide');
                                        $("#rowQuestion_" + queId).remove();
                                    }
                                })
                            }
                            var DeleteAnswer = function () {
                                $("#loaderAnswerDiv").show();
                                var answerId = $("#hiddenAnswerId").val();
                                $.ajax({
                                    type: "POST",
                                    url: "/User/DeleteAnswer",
                                    data: { Id: answerId },
