                        <script>
                            var ConfirmDeleteComment = function (commentId) {
                                $("#hiddenCommentId").val(commentId);
                                $("#CommentModel").modal('show');

                            }
                            var ConfirmDeleteAnswer = function (answerId) {
                                $("#hiddenAnswerId").val(answerId);
                                $("#AnswerModal").modal('show');

                            }
                            var ConfirmDeleteQuestion = function (questionId) {
                                $("#hiddenQuestionId").val(questionId);
                                $("#QuestionModal").modal('show');

                            }

